@model ScheduleClient.ViewModels.AccountRegisterViewModel
@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cadastro</h2>

@using (Html.BeginForm())
{
    <h4>Complete o Cadastro de sua conta, para concluir  agendamento!</h4>
    <hr />

    @Html.ValidationSummary("",new  { @class = "text-danger"}) <!--classe apara validar os requireds-->

    @Html.EditorForModel() <!--asp net cuidará de criar os campor conforme minha model-->

    <div class="form-group">
        <div class="col-md-offset-2 col-md -10">
            <input type="submit" value="Salvar" class="btn-btn-default" />
        </div>
    </div>

}





